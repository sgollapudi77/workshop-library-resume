{
  "routes": [
    {
      "route": "/api/projects/*",
      "methods": ["GET", "PUT", "POST", "DELETE"],
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/*.{css,scss,js,png,gif,ico,jpg}"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "platform": {
    "apiRuntime":"node:16"
  }
}
